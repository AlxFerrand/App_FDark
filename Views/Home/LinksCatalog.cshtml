@model List<Links>;
@{
    ViewData["Title"] = ViewData["CategorySelect"];
}
<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
    <div class="container-fluid">
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
            <ul class="navbar-nav flex-grow-1">
                <li class="nav-item">
                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-route-catId="@ViewBag.ActualCatId" asp-route-subCatId="0" asp-action="LinksCatalog">All</a>
                </li>
                @foreach (SubCategories subCategory in ViewBag.SubCategoriesList)
                {
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-route-catId="@ViewBag.ActualCatId" asp-route-subCatId="@subCategory.Id" asp-action="LinksCatalog">@subCategory.Name</a>
                    </li>
                }
            </ul>
        </div>
    </div>
</nav>
<div class="row">
    <ul>
        @foreach (Links link in Model)
        {
            <li>
                @link.Label
            </li>
        }
    </ul>
</div>
